{
  "name": "natively",
  "version": "1.2.2",
  "scripts": {
    "dev": "expo start --port 8082 --clear",
    "dev:tunnel": "expo start --clear --tunnel",
    "dev:lan": "expo start --clear --lan",
    "dev:force-lan": "cross-env REACT_NATIVE_PACKAGER_HOSTNAME=192.168.100.32 expo start --dev-client --lan",
    "dev:localhost": "expo start --clear --localhost",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --clear",
    "build:web": "expo export -p web && powershell -ExecutionPolicy Bypass -Command \"Copy-Item vercel.json dist\\vercel.json -Force; if (Test-Path public\\manifest.json) { Copy-Item public\\manifest.json dist\\manifest.json -Force }; if (Test-Path public\\sw.js) { Copy-Item public\\sw.js dist\\sw.js -Force }; Copy-Item assets\\images\\icon-prod.png dist\\icon-prod.png -Force; Copy-Item assets\\images\\icon-prod.png dist\\favicon.ico -Force; Get-ChildItem -Path dist\\assets -Filter *.ttf -Recurse | Copy-Item -Destination dist -Force; Get-ChildItem -Path dist -Recurse -Include *.html,*.js | ForEach-Object { $path = $_.FullName; $c = Get-Content -LiteralPath $path -Raw; $c = $c -replace '/assets/node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/', '/'; if ($path.EndsWith('.html')) { if ($c -notlike '*rel=\\\"manifest\\\"*') { $c = $c -replace '<head>', '<head><link rel=\\\"manifest\\\" href=\\\"/manifest.json\\\"><meta name=\\\"apple-mobile-web-app-capable\\\" content=\\\"yes\\\"><meta name=\\\"apple-mobile-web-app-status-bar-style\\\" content=\\\"default\\\"><link rel=\\\"apple-touch-icon\\\" href=\\\"/icon-prod.png\\\">' } else { $c = $c -replace 'link rel=\\\"apple-touch-icon\\\" href=\\\"/logo192x192.png\\\"', 'link rel=\\\"apple-touch-icon\\\" href=\\\"/icon-prod.png\\\"' } }; [System.IO.File]::WriteAllText($path, $c) }\"",
    "build:android": "expo prebuild -p android",
    "lint": "eslint .",
    "update:production": "npx eas-cli@latest update --channel production",
    "update:preview": "npx eas-cli@latest update --channel preview",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.26.9",
    "@expo/metro-runtime": "~3.2.3",
    "@expo/ngrok": "^4.1.0",
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "@supabase/supabase-js": "^2.43.4",
    "@types/difflib": "^0.2.7",
    "base-64": "^1.0.0",
    "base64-arraybuffer": "^1.0.2",
    "blob-polyfill": "^9.0.20240710",
    "difflib": "^0.2.4",
    "expo": "~51.0.28",
    "expo-blur": "~13.0.3",
    "expo-build-properties": "~0.12.0",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-keep-awake": "~13.0.2",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-notifications": "~0.28.9",
    "expo-pwa": "^0.0.58",
    "expo-router": "~3.5.16",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-symbols": "~0.1.5",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.280.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-blob-util": "^0.19.9",
    "react-native-css-interop": "^0.1.22",
    "react-native-drawer-layout": "^4.2.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-google-mobile-ads": "14.2.3",
    "react-native-html-to-pdf": "^0.12.0",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.7.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.0",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "ngrok": "^5.0.0-beta.2",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "react-router-dom": "^7.1.3",
    "react-test-renderer": "18.2.0",
    "tunnel": "^0.0.6",
    "typescript": "~5.3.3",
    "webpack-cli": "^6.0.1",
    "workbox-cli": "^2.1.3",
    "workbox-precaching": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "private": true
}