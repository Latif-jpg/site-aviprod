{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ID": "com.aviprodapp.app.dev"
      }
    },
    "development-fresh": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "24.12.0",
      "env": {
        "APP_ID": "com.aviprodapp.app.fresh"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}